

#include <stdio.h>
#include <stdlib.h>
	int main(void)
	{
	int sz,i;
	int randArray[10];
	 printf("Enter the size of array::");
	scanf("%d",&sz);
   		for(i=0;i<sz;i++)
     		randArray[i]=rand()%100;   //Generate number between 0 to 99
 		  printf("\nElements of the array::");
   
	for(i=0;i<sz;i++)
   	{
     		printf("\nElement number %d::%d",i+1,randArray[i]);
  	 }
  	 return 0;
}